msgid "Overlay Backup"
msgstr "Overlay 备份"

msgid "Overlay Restore"
msgstr "Overlay 恢复"

msgid "Overlay Backup Settings"
msgstr "Overlay 备份设置"

msgid "Backup"
msgstr "备份"

msgid "Restore"
msgstr "恢复"

msgid "Settings"
msgstr "设置"

msgid "Log"
msgstr "日志"

msgid "Create Backup"
msgstr "创建备份"

msgid "Create a backup of the current overlay filesystem, including installed packages list and system configuration."
msgstr "创建当前 overlay 文件系统的备份，包括已安装的软件包列表和系统配置。"

msgid "Backup Path"
msgstr "备份路径"

msgid "Select the directory to save the backup file."
msgstr "选择保存备份文件的目录。"

msgid "Backup Filename"
msgstr "备份文件名"

msgid "The backup file will be named according to system version and current time."
msgstr "备份文件将根据系统版本和当前时间命名。"

msgid "Existing Backups"
msgstr "已有备份"

msgid "List of backup files in the selected directory."
msgstr "所选目录中的备份文件列表。"

msgid "Filename"
msgstr "文件名"

msgid "Size"
msgstr "大小"

msgid "Date"
msgstr "日期"

msgid "Actions"
msgstr "操作"

msgid "Download"
msgstr "下载"

msgid "Delete"
msgstr "删除"

msgid "Loading..."
msgstr "加载中..."

msgid "No backup files found."
msgstr "未找到备份文件。"

msgid "Creating Backup"
msgstr "正在创建备份"

msgid "Please wait while the backup is being created..."
msgstr "正在创建备份，请稍候..."

msgid "Backup Complete"
msgstr "备份完成"

msgid "Backup created successfully: "
msgstr "备份创建成功："

msgid "Backup Failed"
msgstr "备份失败"

msgid "Unknown error occurred."
msgstr "发生未知错误。"

msgid "Error"
msgstr "错误"

msgid "Are you sure you want to delete this backup file?"
msgstr "确定要删除此备份文件吗？"

msgid "Processing"
msgstr "处理中"

msgid "Please wait..."
msgstr "请稍候..."

msgid "Close"
msgstr "关闭"

msgid "Upload Backup File"
msgstr "上传备份文件"

msgid "Upload a backup file from your computer to restore."
msgstr "从您的计算机上传备份文件以进行恢复。"

msgid "Select File"
msgstr "选择文件"

msgid "Select a backup file (.tar.gz) to upload."
msgstr "选择要上传的备份文件 (.tar.gz)。"

msgid "Upload"
msgstr "上传"

msgid "Select Backup to Restore"
msgstr "选择要恢复的备份"

msgid "Select a backup file from the list below to restore."
msgstr "从下面的列表中选择要恢复的备份文件。"

msgid "Search Path"
msgstr "搜索路径"

msgid "No backup files found. Please upload a backup file first."
msgstr "未找到备份文件。请先上传备份文件。"

msgid "Backup Information"
msgstr "备份信息"

msgid "Restore Selected Backup"
msgstr "恢复所选备份"

msgid "Please select a file to upload."
msgstr "请选择要上传的文件。"

msgid "Invalid file format. Please select a .tar.gz backup file."
msgstr "无效的文件格式。请选择 .tar.gz 备份文件。"

msgid "File uploaded successfully: "
msgstr "文件上传成功："

msgid "Upload failed: "
msgstr "上传失败："

msgid "Upload failed: Unknown error"
msgstr "上传失败：未知错误"

msgid "Upload failed: Network error"
msgstr "上传失败：网络错误"

msgid "Please select a backup file to restore."
msgstr "请选择要恢复的备份文件。"

msgid "Confirm Restore"
msgstr "确认恢复"

msgid "Warning: This operation will restore the overlay filesystem and reboot the system."
msgstr "警告：此操作将恢复 overlay 文件系统并重启系统。"

msgid "Selected backup: "
msgstr "已选择的备份："

msgid "Are you sure you want to continue?"
msgstr "确定要继续吗？"

msgid "Cancel"
msgstr "取消"

msgid "Confirm Restore"
msgstr "确认恢复"

msgid "Restoring"
msgstr "正在恢复"

msgid "Restoring backup and preparing to reboot..."
msgstr "正在恢复备份并准备重启..."

msgid "Please wait and do not close this page."
msgstr "请等待，不要关闭此页面。"

msgid "Restore Complete"
msgstr "恢复完成"

msgid "Restore completed successfully. The system is rebooting..."
msgstr "恢复成功完成。系统正在重启..."

msgid "Please wait for the system to restart and then refresh this page."
msgstr "请等待系统重启，然后刷新此页面。"

msgid "Restore completed successfully. Please reboot the system manually to apply changes."
msgstr "恢复成功完成。请手动重启系统以应用更改。"

msgid "Restore Failed"
msgstr "恢复失败"

msgid "Backup/Restore Log"
msgstr "备份/恢复日志"

msgid "Operation Log"
msgstr "操作日志"

msgid "View the log of backup and restore operations."
msgstr "查看备份和恢复操作的日志。"

msgid "Log Type"
msgstr "日志类型"

msgid "Backup Log"
msgstr "备份日志"

msgid "Restore Log"
msgstr "恢复日志"

msgid "Refresh"
msgstr "刷新"

msgid "Clear Log"
msgstr "清除日志"

msgid "No log entries found."
msgstr "未找到日志条目。"

msgid "Failed to load log."
msgstr "加载日志失败。"

msgid "Are you sure you want to clear the log?"
msgstr "确定要清除日志吗？"

msgid "Log cleared."
msgstr "日志已清除。"

msgid "General Settings"
msgstr "常规设置"

msgid "Configure backup and restore settings for overlay filesystem."
msgstr "配置 overlay 文件系统的备份和恢复设置。"

msgid "Default Backup Path"
msgstr "默认备份路径"

msgid "Default directory for storing backup files."
msgstr "存储备份文件的默认目录。"

msgid "Auto Reboot After Restore"
msgstr "恢复后自动重启"

msgid "Automatically reboot the system after restore completes."
msgstr "恢复完成后自动重启系统。"

msgid "LuCI Overlay Backup and Restore"
msgstr "LuCI Overlay 备份和恢复"

msgid "A LuCI application for backing up and restoring the overlay filesystem"
msgstr "用于备份和恢复 overlay 文件系统的 LuCI 应用程序"
